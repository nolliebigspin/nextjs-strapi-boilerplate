{
  "name": "nextjs-strapi-boilerplate",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.16.0"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "1.22.19"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo build",
    "dev": "dotenv -- turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv-cli": "^7.0.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "^1.10.2"
  },
  "resolutions": {
    "resolve": "<1.21.0",
    "graphql": "^15.5.1"
  }
}
